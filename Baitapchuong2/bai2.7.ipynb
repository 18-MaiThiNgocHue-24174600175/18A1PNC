    "import json\n",
    "json_data = '''\n",
    "{\n",
    "    \"name\": \"Nguyễn Văn A \",\n",
    "    \"age\": 21,\n",
    "    \"city\": \"Hà Nội\"\n",
    "}\n",
    "'''\n",
    "data = json.loads(json_data)\n",
    "\n",
    "print(\"Họ tên:\", data[\"name\"])\n",
    "print(\"Tuổi:\", data[\"age\"])\n",
    "print(\"Thành phố:\", data[\"city\"])"
   ]
  }
